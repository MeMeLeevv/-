
.body{
    display: flex;
  justify-content: space-around;
  box-sizing: border-box;
}
.item{
  width: 48%;
  display: inline-block;
  margin: 1%;
  padding: 1%;
  box-sizing: border-box;
}
image.pic{
  width: 100%;
  height: 180px;
  border-radius: 10px;
}
text{
  display: block;
  font-size: 14px;
  font-weight: bold;
  margin:  10rpx;
}
.searchPlace{
  width: 100%;
  height: 100rpx;
}
.searchIcon{
  width: 100rpx;
  height: 100rpx;
  float: right;
}